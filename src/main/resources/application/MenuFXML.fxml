<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<AnchorPane styleClass="Main" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.jocomol.javamon.Controller">
   <children>



      <Pane layoutX="14.0" layoutY="-18.0" prefHeight="1042.0" prefWidth="1904.0">
         <children>

            <ImageView fitHeight="327.0" fitWidth="1070.0" layoutX="480.0" layoutY="89.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="file:logo.png"/>
               </image>
            </ImageView>
            <Button fx:id="play_button" layoutX="735.0" layoutY="379.0" mnemonicParsing="false" prefHeight="120.0" prefWidth="460.0" text="Play" onAction="#gamePlay">
               <font>
                  <Font size="40.0" />
               </font>
            </Button>
            <Button fx:id="shop_button" layoutX="734.0" layoutY="516.0" mnemonicParsing="false" prefHeight="120.0" prefWidth="460.0" text="Shop">
               <font>
                  <Font size="40.0" />
               </font>
            </Button>
            <Button fx:id="options_button" layoutX="737.0" layoutY="658.0" mnemonicParsing="false" onAction="#callOptions" prefHeight="120.0" prefWidth="460.0" text="Options">
               <font>
                  <Font size="40.0" />
               </font>
            </Button>
            <Button fx:id="volume_button" layoutX="1817.0" layoutY="1020.0" mnemonicParsing="false" text="" onAction="#volumeSwitch"/>

            <Button fx:id="quit_button" layoutX="737.0" layoutY="797.0" mnemonicParsing="false" onAction="#quitGame" prefHeight="120.0" prefWidth="460.0" text="Quit">
               <font>
                  <Font size="40.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </children>
</AnchorPane>
